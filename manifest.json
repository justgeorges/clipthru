{
  "manifest_version": 3,
  "name": "clipthru",
  "version": "0.1",
  "description": "save an organize web content the way you want to",
  "permissions": ["activeTab", "storage"],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["clip.js"]
    }
  ],
  "icons": {
    "48": "icon.png"
  }
}
